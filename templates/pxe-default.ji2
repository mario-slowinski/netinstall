default vesamenu.c32

timeout 300
totaltimeout 6000

menu clear
menu background splash.png
menu title PRS PXE installer
menu vshift 8
menu rows 18
menu margin 8
#menu hidden
menu helpmsgrow 15
menu tabmsgrow 13

{% for system in os %}
include pxelinux.cfg/{{ system['name'] }} {{ system['description'] }}
{% endfor %}

menu separator
label local
        menu label Boot from local drive
        menu default
        localboot 0
